{
  "scripts": {
    "test": "jest --runInBand && jest",
    "server": "nodemon server.js",
    "web-server": "dotenv -e ~/barcode-env -- docker-compose -f local-web-server/docker-compose.yml up"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.5.0",
    "@bbob/core": "^2.6.2",
    "axios": "^0.21.1",
    "better-sqlite3": "^7.1.2",
    "bree": "^6.0.0",
    "console-stamp": "^0.2.9",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.16.1",
    "date-fns-tz": "^1.0.12",
    "debug": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "follow-redirects": "^1.13.2",
    "handlebars": "^4.7.6",
    "lodash": "^4.17.20",
    "ms": "^2.1.3",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-strategy": "^1.0.0",
    "proper-lockfile": "^4.1.2",
    "sharp": "^0.27.1",
    "ssh2": "^0.8.9"
  },
  "devDependencies": {
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.10.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "xo": "^0.38.2"
  },
  "jest": {
    "testEnvironment": "node",
    "silent": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "xo": {
    "space": 4,
    "rules": {
      "unicorn/prevent-abbreviations": 0,
      "unicorn/no-array-for-each": 0,
      "unicorn/no-lonely-if": 0,
      "unicorn/prefer-number-properties": 0,
      "unicorn/prefer-array-find": 0,
      "unicorn/no-process-exit": 0,
      "unicorn/no-array-reduce": 0,
      "unicorn/prefer-string-slice": 0,
      "capitalized-comments": 0,
      "key-spacing": 0,
      "no-await-in-loop": 0,
      "padding-line-between-statements": 0,
      "padded-blocks": 0,
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "new-cap": 0
    },
    "overrides": [
      {
        "files": "tests/*",
        "globals": [
          "jest",
          "describe",
          "it",
          "beforeAll",
          "afterAll",
          "expect"
        ]
      }
    ]
  }
}
