{
  "scripts": {
    "test": "jest --runInBand && jest",
    "server": "nodemon server.js",
    "web-server": "dotenv -e ~/barcode-env -- docker-compose -f local-web-server/docker-compose.yml up"
  },
  "dependencies": {
    "@aws-sdk/client-sns": "^3.43.0",
    "@bbob/core": "^2.8.0",
    "@googlemaps/google-maps-services-js": "^3.3.4",
    "axios": "^0.21.4",
    "better-sqlite3": "^7.4.5",
    "bree": "^6.5.0",
    "console-stamp": "^3.0.3",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.27.0",
    "date-fns-tz": "^1.1.6",
    "debug": "^4.3.3",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "follow-redirects": "^1.14.5",
    "fuse.js": "^6.4.6",
    "handlebars": "^4.7.6",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "multer": "^1.4.3",
    "mysql2": "^2.3.3",
    "ordinal": "^1.0.3",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-strategy": "^1.0.0",
    "proper-lockfile": "^4.1.2",
    "sharp": "^0.27.2",
    "ssh2": "^1.5.0"
  },
  "devDependencies": {
    "dotenv-cli": "^4.1.1",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.15",
    "supertest": "^6.1.6",
    "xo": "^0.44.0"
  },
  "jest": {
    "testEnvironment": "node",
    "silent": true,
    "globalSetup": "./tests/global-setup.js",
    "globalTeardown": "./tests/global-teardown.js",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "xo": {
    "space": 4,
    "rules": {
      "unicorn/prevent-abbreviations": 0,
      "unicorn/no-array-for-each": 0,
      "unicorn/no-lonely-if": 0,
      "unicorn/prefer-number-properties": 0,
      "unicorn/prefer-array-find": 0,
      "unicorn/no-process-exit": 0,
      "unicorn/no-array-reduce": 0,
      "unicorn/prefer-string-slice": 0,
      "unicorn/prefer-module": 0,
      "unicorn/numeric-separators-style": 0,
      "capitalized-comments": 0,
      "key-spacing": 0,
      "no-await-in-loop": 0,
      "padding-line-between-statements": 0,
      "padded-blocks": 0,
      "brace-style": [
        "error",
        "stroustrup"
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "new-cap": 0,
      "import/extensions": 0,
      "comma-dangle": 0,
      "node/prefer-global/process": 0,
      "operator-linebreak": 0,
      "unicorn/prefer-object-from-entries": 0
    },
    "overrides": [
      {
        "files": "tests/*",
        "rules": {
          "import/order": 0
        },
        "globals": [
          "jest",
          "describe",
          "it",
          "beforeAll",
          "afterAll",
          "expect",
          "beforeEach"
        ]
      }
    ]
  }
}
